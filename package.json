{
	"name": "idlweb",
	"description": "Interactive Data Lab Website",
	"homepage": "http://uwdata.github.io/",
	"version": "2.0.0",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"dev": "vite dev",
		"build": "npm run integrity-enforcement && vite build && cp 404.html build/404.html",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"prepare": "husky",
		"test": "npm run check && npm run integrity-enforcement && vitest run",
		"test:watch": "vitest",
		"integrity-enforcement": "vite-node scripts/integrity-enforcement.ts "
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/d3-force": "^3.0.9",
		"@types/eslint": "^8.56.0",
		"@types/markdown-it": "^14.0.1",
		"@types/node": "^20.12.5",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"@vitest/ui": "^1.5.2",
		"ajv": "^8.12.0",
		"autoprefixer": "^10.4.19",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"husky": "^9.0.11",
		"postcss": "^8.4.38",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.3",
		"ts-json-schema-generator": "^1.5.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"vitest": "^1.5.2"
	},
	"type": "module",
	"dependencies": {
		"@types/d3": "^7.4.3",
		"d3": "^7.9.0",
		"d3-force": "^3.0.0",
		"markdown-it": "^14.1.0"
	}
}
