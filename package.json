{
  "name": "uwdata.github.io",
  "description": "Interactive Data Lab Website",
  "homepage": "http://idl.uw.edu/",
  "version": "2.0.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "vite dev",
    "build": "npm run integrity-enforcement && vite build && cp 404.html build/404.html",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "prepare": "husky",
    "test": "npm run check && npm run integrity-enforcement && vitest run",
    "test:watch": "vitest",
    "integrity-enforcement": "vite-node scripts/integrity-enforcement.ts "
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@sveltejs/adapter-auto": "^3.2.2",
    "@sveltejs/adapter-static": "^3.0.2",
    "@sveltejs/kit": "^2.5.17",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@types/d3-force": "^3.0.10",
    "@types/eslint": "^8.56.10",
    "@types/markdown-it": "^14.1.1",
    "@types/node": "^20.14.7",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitest/ui": "^1.6.0",
    "ajv": "^8.16.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-svelte": "^2.45.1",
    "globals": "^15.11.0",
    "husky": "^9.0.11",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.5",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.1",
    "tailwindcss": "^3.4.4",
    "ts-json-schema-generator": "^1.5.0",
    "tslib": "^2.6.3",
    "typescript": "^5.5.2",
    "typescript-eslint": "^8.10.0",
    "vite": "^5.3.1",
    "vitest": "^1.6.0"
  },
  "type": "module",
  "dependencies": {
    "@types/d3": "^7.4.3",
    "d3": "^7.9.0",
    "d3-force": "^3.0.0",
    "markdown-it": "^14.1.0"
  }
}
